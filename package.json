{
  "name": "ticketsystem",
  "version": "1.0.0",
  "description": "Ein umfassendes Ticketsystem fÃ¼r Kundensupport",
  "main": "server/index.js",
  "scripts": {
    "dev": "concurrently -n server,client -c green,blue \"npm run server:dev\" \"npm run client:dev\"",
    "server:dev": "cd server && npm run dev",
    "client:dev": "cd client && npm run dev",
    "build": "cd client && npm run build",
    "start": "cd server && npm start",
    "install:all": "npm install && cd server && npm install && cd ../client && npm install",
    "start:multitenant": "cd server && npm run multitenant"
  },
  "keywords": ["ticket", "support", "customer-service"],
